#include <stdio.h>

int main() {
    int arr[] = {10, 20, 30, 40, 50}; 
    int n = sizeof(arr) / sizeof(arr[0]); 
    int sum = 0; 
    float avg; 

    
    for (int i = 0; i < n; i++) {
        sum += arr[i];
    }

    
    avg = (float) sum / n;

    printf("Sum of the elements: %d\n", sum);
    printf("Average of the elements: %.2f\n", avg);

    return 0;
}
